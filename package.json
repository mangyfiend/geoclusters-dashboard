{
  "name": "avg-dashboard-ssr",
  "version": "1.1.0",
  "description": "NIRSAL AVG Dashboard - SSR Beta V1.1.0",
  "main": "express.js",
  "scripts": {
    "start": "cd server && node server.js",
    "dev": "cd server && nodemon server.js --ignore localdata/ --ignore public/dist/",
    "build": "webpack --mode production",
    "secure": "node obfuscate.js",
    "build:secure": "npm-run-all -s build secure",
    "deploy": "git push && git push heroku master && heroku open",
    "deploy:prod": "set NODE_ENV=production && webpack --mode production && node obfuscate.js && git add -A && git commit -m `stable-commit` && git push && git push heroku master",
    "stable": "git add -A && git commit -m `stable-commit`",
    "on": "heroku maintenance:on",
    "off": "heroku maintenance:off"
  },
  "nodemonConfig": {
    "ignore": [
      [
        "/localdata/*.json",
        "/public/dist/"
      ]
    ]
  },
  "author": "Nduka Okpue",
  "license": "ISC",
  "dependencies": {
    "@mapbox/togeojson": "^0.16.0",
    "@turf/turf": "^5.1.6",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^5.3.0",
    "express-session": "^1.17.2",
    "geojson-validation": "^1.0.2",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.startcase": "^4.4.0",
    "mongoose": "^5.9.19",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pug": "^3.0.0",
    "validator": "^13.1.1",
    "winston": "^3.3.3",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "css-loader": "^6.4.0",
    "file-loader": "^6.2.0",
    "javascript-obfuscator": "^2.6.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.3.0",
    "webpack": "^5.59.0",
    "webpack-cli": "^4.9.1"
  },
  "engines": {
    "node": "^12"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/monolithgoon/nirsal-avg-dashboard.git"
  },
  "keywords": [
    "dashboard",
    "geospatial"
  ],
  "bugs": {
    "url": "https://github.com/monolithgoon/nirsal-avg-dashboard/issues"
  },
  "homepage": "https://github.com/monolithgoon/nirsal-avg-dashboard#readme"
}
